{
  "name": "Quill Autoanswer Hack",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Simple js injector extension to inject my quil-autoanswer-hack",
  "homepage_url": "https://github.com/Element21/quil-autoanswer-hack",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options_custom/index.html",
  "action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "background": "background-worker.js",
  "permissions": [
    "cookies",
    "webNavigation"
  ],
  "host_permissions": [
    "*://quill.org/*"
  ]
}